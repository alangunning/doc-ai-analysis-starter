{
  "name": "doc-ai-starter-docs",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "docusaurus start",
    "build": "npm run lint:alt-text && docusaurus build && npm run test:a11y",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "lint:alt-text": "bash -c \"if rg -n '!\\[]\\(' content; then echo 'Images missing alt text'; exit 1; fi\"",
    "test:a11y": "pa11y file://$PWD/build/index.html"
  },
  "dependencies": {
    "@docusaurus/core": "^3.3.2",
    "@docusaurus/preset-classic": "^3.3.2",
    "@docusaurus/theme-mermaid": "^3.3.2",
    "dotenv": "^16.4.5",
    "prism-react-renderer": "^2.3.0"
  },
  "devDependencies": {
    "pa11y": "^9.0.0"
  }
}
